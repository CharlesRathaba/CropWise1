{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "dest": "/api/analyze.js"
    },
    {
      "src": "/download",
      "dest": "/api/download.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
} 